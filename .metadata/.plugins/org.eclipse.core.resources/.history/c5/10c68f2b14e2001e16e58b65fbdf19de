package M_Q_Class_Object;

public class Qution_10 {

	public static void main(String[] args)
	{
		
		Qution_10 q = new Qution_10();
		q.method(30);
		byte b=3;
		q.method(b);
		
	}
	
	public void method(integer i)
	{
		System.out.println("Integer value id:"+i+"");
	}
	
	public void method(shoet s)
	{
		System.out.println("Integer value id:"+s+"");
	}
	
	public void method(byte t)
	{
		System.out.println("Integer value id:"+t+"");
	}
	
	public void method(int num)
	{
		System.out.println("Integer value id:"+num+"");
	}

}

// in above code ther is integer and short is not data type 
// intgeer is class name and short is keyward
